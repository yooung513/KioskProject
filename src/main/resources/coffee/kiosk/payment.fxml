<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="900.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="coffee.kiosk.Controller.PaymentController">
   <children>
      <VBox prefHeight="900.0" prefWidth="500.0">
         <children>
            <Button fx:id="cardbutton" mnemonicParsing="false" prefHeight="250.0" prefWidth="300.0" style="-fx-background-color: #F9E10E;" text="카드 결제">
               <VBox.margin>
                  <Insets left="100.0" top="150.0" />
               </VBox.margin>
               <font>
                  <Font name="카페24 슈퍼매직 Bold" size="48.0" />
               </font>
            </Button>
            <Button fx:id="cashbutton" mnemonicParsing="false" prefHeight="250.0" prefWidth="300.0" style="-fx-background-color: #F9E10E;" text="현금 결제">
               <VBox.margin>
                  <Insets left="100.0" top="50.0" />
               </VBox.margin>
               <font>
                  <Font name="카페24 슈퍼매직 Bold" size="48.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
